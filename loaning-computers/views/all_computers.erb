<%if computers == nil%>
    <p>Datorn skaffa vingar och flög iväg</p>
    <a href="/">Tillbaka<a>
<%else%>
    <table>
        <thead>
            <th class="text">ID</th>
            <th class="text">Serial No</th>
            <th class="text">Student ID</th>
        </thead>
        <tbody>
        <% i= 0
        while computers.length > i%>
            <tr>
                <td class="text"><%= computers[i][0] %></td>
                <td class="text"><%= computers[i][1] %></td>
                <td class="text"><%= computers[i][3] %></td>
            </tr>
            <% i+=1 
            end %>
        </tbody>
    </table>
    <a href="/">Tillbaka</a>
<%end%>